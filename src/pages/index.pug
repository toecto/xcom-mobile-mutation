                            var slider_change_time = 5000;
                            var numSlides = 0;
                            var dotWidth = 30;
                            var sliderTimer = false;
                            var sliderDrawDots = false;
                            function animate_slide() {
                            if (newPos == curPos) {
                            return
                            }
                            stop_slider_timer();
                            if (sliderDrawDots) $('#dot' + curPos).removeClass('dot_selected');
                            $('#slideInner').stop(true);
                            if ((curPos == numSlides - 1) && (newPos == 0)) {
                            $('#slideInner').animate({
                            'marginLeft': -sBannerWidth * (numSlides + 1)
                            }, {
                            duration: 400,
                            complete: function () {
                            $('#slideInner').css('margin-left', -sBannerWidth + 'px');
                            reset_slider_timer();
                            }
                            });
                            } else if ((curPos == 0) && (newPos == numSlides - 1)) {
                            $('#slideInner').animate({
                            'marginLeft': 0
                            }, {
                            duration: 400,
                            complete: function () {
                            $('#slideInner').css('margin-left', -sBannerWidth * numSlides + 'px');
                            reset_slider_timer();
                            }
                            });
                            } else {
                            if (Math.abs(newPos - curPos) > 1) {
                            $('#slideInner').animate({
                            'marginLeft': -sBannerWidth * (newPos + 1)
                            }, {
                            duration: 400 + Math.abs(newPos - curPos) * 100,
                            complete: reset_slider_timer
                            });
                            } else {
                            $('#slideInner').animate({
                            'marginLeft': -1 * sBannerWidth * (newPos + 1)
                            }, {
                            duration: 400,
                            complete: reset_slider_timer
                            });
                            }
                            }
                            curPos = newPos;
                            if (sliderDrawDots) $('#dot' + curPos).addClass('dot_selected');
                            }
                            function next_slide() {
                            if (curPos == numSlides - 1) {
                            newPos = 0
                            } else {
                            newPos = curPos + 1
                            }
                            animate_slide();
                            }
                            function prev_slide() {
                            if (curPos == 0) {
                            newPos = numSlides - 1
                            } else {
                            newPos = curPos - 1
                            }
                            animate_slide();
                            }
                            function reset_slider_timer() {
                            stop_slider_timer();
                            start_slider_timer();
                            }
                            function stop_slider_timer() {
                            clearTimeout(sliderTimer);
                            sliderTimer = false;
                            }
                            function start_slider_timer() {
                            if (sliderTimer === false) {
                            sliderTimer = setTimeout(next_slide, slider_change_time);
                            }
                            }
                            function create_slide(slide, n) {
                            return '<div class="slide" id="slide' + n + '" style="background-color: transparent"><a href="' + slide
                            .link +
                            '"><img src="' + slide.image + '"></a></div>'
                            }
                            function create_slide_dot(n) {
                            return '<div class="dot" id="dot' + n + '" data-num="' + n + '"></div>'
                            }
                            function parse_date(date_s) {
                            var date_c = date_s.split('-');
                            return new Date(date_c[0], date_c[1] - 1, date_c[2]);
                            }
                            function create_slides() {
                            sliderbanners = sliderbanners.filter(function (slide) {
                            if (slide.hidden) return false;
                            if (slide.expire) slide.date_to = slide.expire;
                            if (slide.date_from)
                            if (Date.now() < parse_date(slide.date_from).getTime()) return false;
                            if (slide.date_to)
                            if (Date.now() > parse_date(slide.date_to).getTime() + 86400000) return false;
                            return true;
                            });
                            numSlides = sliderbanners.length;
                            $('#slideInner').append(create_slide(sliderbanners[sliderbanners.length - 1], 999991));
                            $.each(sliderbanners, function (i, slide) {
                            $('#slideInner').append(create_slide(slide, i));
                            if (sliderDrawDots) $('#dotsContainer').append(create_slide_dot(i));
                            });
                            $('#slideInner').append(create_slide(sliderbanners[0], 999992));
                            $('#slideInner').css('margin-left', -sBannerWidth + 'px');
                            if (sliderDrawDots) $('#dot0').addClass('dot_selected');
                            $('#slideInner').css('width', ((numSlides + 2) * sBannerWidth) + 'px');
                            }
                            $(document).ready(function () {
                            create_slides();
                            if (sliderDrawDots) $('.dot').bind('click', function (e) {
                            e.stopPropagation();
                            newPos = parseInt($(this).data('num'));
                            animate_slide();
                            });
                            $('.sliderShadow.left').bind('click', function (e) {
                            e.stopPropagation();
                            prev_slide();
                            });
                            $('.sliderShadow.right').bind('click', function (e) {
                            e.stopPropagation();
                            next_slide();
                            });
